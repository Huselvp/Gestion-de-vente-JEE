<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "https://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="etu.uae.gestion_vente.entities.Article" table="Article_Prix">
        <id name="codeArt" column="codeArt" type="int">
            <generator class="identity" />
        </id>
        <property name="nom" column="nomArt" type="java.lang.String"/>
        <property name="prix" column="prixArt" type="java.lang.Integer"/>
        <property name="description" column="descArt" type="java.lang.String"/>

    </class>
</hibernate-mapping>
